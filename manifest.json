{
  "name": "Debar",
  "version": "0.0.1",
  "description": "You can disable \"bottom control bar\" in Google Presentation now.",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    { "matches": ["https://docs.google.com/presentation/d/*"], "js": ["debar.js"],
      "run_at": "document_idle"}
  ],
  "permissions": [
  ],
  "manifest_version": 2,
  "content_security_policy": "default-src 'none'; style-src 'self'; script-src 'self';"
}
